{
	"name": "obsidian-hanayama-huzzles-tracker-plugin",
	"version": "1.0.0",
	"description": "This is a Hanayama Huzzle puzzles tracker plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"hanayama",
		"huzzle",
		"puzzle",
		"tracker"
	],
	"author": "Iulian Onofrei",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"dedent": "1.5.1",
		"esbuild": "0.17.3",
		"escape-string-regexp": "5.0.0",
		"eslint": "8.56.0",
		"obsidian": "latest",
		"remark": "15.0.1",
		"remark-gfm": "4.0.0",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
